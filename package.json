    "dotenv": "^6.0.0",
    "eslint-plugin-react": "^7.8.2",
    "express": "^5.0.0-alpha.6",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "http-status-codes": "^1.3.0",
    "joi": "^13.2.0",
    "jsonwebtoken": "^8.2.1",
    "knex": "^0.15.0",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "path": "^0.12.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
